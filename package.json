{
  "name": "revolut-fxform",
  "version": "0.0.1",
  "description": "A web-based Revolut currency exchange form",
  "main": "server/app.js",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production & webpack -p --progress --config webpack.config.js & node server/app.js",
    "test": "echo \"Error: no unit tests yet...\" && exit 1",
    "build": "webpack --progress -p --config webpack.config.js",
    "buildDebug": "webpack --progress -d --config webpack.config.dev.js",
    "debug": "webpack -d --progress --watch --config webpack.config.dev.js & ./node_modules/.bin/nodemon ./server/app.js | bunyan",
    "debugServer": "./node_modules/.bin/nodemon ./server/app.js"
  },
  "keywords": [
    "revolut",
    "currency",
    "fx",
    "money"
  ],
  "author": "Romas Kriauciukas <romas.kriauciukas@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "bunyan": "^1.8.1",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "currency-formatter": "^1.5.5",
    "express": "^4.14.0",
    "immutable": "^3.8.1",
    "jquery": "^3.5.0",
    "json-loader": "^0.5.7",
    "libxmljs": "^0.18.0",
    "lodash": "^4.15.0",
    "memory-cache": "^0.1.6",
    "node-sass-middleware": "^0.9.8",
    "pug": "^2.0.0-beta6",
    "q": "^1.4.1",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.1",
    "reflux": "^0.4.1",
